   "source": [
    "N = int(input(\"Please enter N :- \"))\n",
    "\n",
    "SUM = 0\n",
    "Number = N\n",
    "\n",
    "\n",
    "while Number != 0:\n",
    "    if Number != 0:\n",
    "        SUM = (Number%10)**3 + SUM\n",
    "        print(Number % 10)\n",
    "        Number = Number // 10\n",
    "        \n",
    "    if N == SUM:\n",
    "        print(f\"N is an armstrong number\")\n",
    "        break\n",
    "print(SUM)"
   ]
