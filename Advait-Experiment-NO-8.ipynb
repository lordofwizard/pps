    "import collections\n",
    "import pprint\n",
    "file_input = input(\"File name :- \")\n",
    "\n",
    "with open(file_input,'r') as info:\n",
    "    count = collections.Counter(info.read().upper())\n",
    "    value = pprint.pformat(count)\n",
    "\n",
    "print(value)"
