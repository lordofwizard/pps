   "source": [
    "decimal = 0\n",
    "base = 1\n",
    "\n",
    "binary = int(input(\"Please input binary (10) no :- \"))\n",
    "while binary != 0:\n",
    "    digit = binary % 10\n",
    "    decimal = digit * base + decimal\n",
    "    base = base * 2\n",
    "    binary = binary // 10\n",
    "\n",
    "print(\"Decimal number of given binary is\",decimal)"
   ]
  }
